{
  "$schema": "http://json­schema.org/draft­04/schema#",
  "id": "http://schemas.zivver.org/contacts/entityContactResponse.json",
  "type": "object",
  "properties": {
    "id": {
      "$ref": "../common/definitions.json#/properties/identifier"
    },
    "email": {
      "$ref": "../common/definitions.json#/properties/emailaddress"
    },
    "firstName": {
      "type": "string",
      "maxLength": 255
    },
    "lastName": {
      "type": "string",
      "maxLength": 255
    },
    "phonenumber": {
      "$ref": "../common/definitions.json#/properties/phonenumber"
    },
    "picture": {
      "type": "string", "maxLength": 255, "format": "uri"
    }
  },
  "required": [ "id", "email", "firstName", "lastName"],
  "additionalProperties": false
}
